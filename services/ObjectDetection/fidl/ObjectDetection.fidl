package commonapi
 
interface ObjectDetection {
  version {major 1 minor 0}

  broadcast ObjectType {
    out { Int32 objtype }
  }

  broadcast ObjectVelocityAxis {
    out { 
	  Float object_velocity_x
	  Float object_velocity_y
	  Float object_velocity_z
    }
  }

  broadcast ObjectAccelAxis {
    out { 
	  Float object_accel_x
	  Float object_accel_y
	  Float object_accel_z
    }
  }

} 

